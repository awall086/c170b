<?xml version="1.0"?>

<PropertyList>

    <path>vac.ac</path>

	<animation>
		<type>material</type>
        <object-name>face</object-name>
        <object-name>needle</object-name>
		<emission>
			<red>0.10</red>
			<green>0.02</green>
			<blue>0.0</blue>
			<factor-prop>/controls/lighting/int-lights</factor-prop>
		</emission>
	</animation>
	
    <animation>
        <type>rotate</type>
        <object-name>needle</object-name>
        <property>systems/vacuum/suction-inhg</property>
        <interpolation>
            <entry><ind>  0.0</ind><dep>-152</dep></entry>
            <entry><ind>  1.0</ind><dep>-120</dep></entry>
            <entry><ind>  2.0</ind><dep> -90</dep></entry>
            <entry><ind>  3.0</ind><dep> -60</dep></entry>
            <entry><ind>  4.0</ind><dep> -30</dep></entry>
            <entry><ind>  5.0</ind><dep>   0</dep></entry>
            <entry><ind>  6.0</ind><dep>  30</dep></entry>
            <entry><ind>  7.0</ind><dep>  60</dep></entry>
            <entry><ind>  8.0</ind><dep>  90</dep></entry>
            <entry><ind>  9.0</ind><dep> 120</dep></entry>
            <entry><ind> 10.0</ind><dep> 152</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>
