<?xml version="1.0"?>

<PropertyList>

 <path>amps.ac</path>

	<animation>
		<type>material</type>
        <object-name>face</object-name>
        <object-name>old_needle</object-name>
		<emission>
			<red>0.10</red>
			<green>0.02</green>
			<blue>0.0</blue>
			<factor-prop>/controls/lighting/int-lights</factor-prop>
		</emission>
	</animation>

	<animation> 
		<type>rotate</type>
		<object-name>old_needle</object-name>
		<property>systems/electrical/bus_load</property>
		<interpolation>
			<entry><ind> -30</ind><dep> -32</dep></entry>
			<entry><ind> -15</ind><dep> -16</dep></entry>
			<entry><ind>   0</ind><dep>   0</dep></entry>
			<entry><ind>  15</ind><dep>  16</dep></entry>
			<entry><ind>  30</ind><dep>  32</dep></entry>
		</interpolation>
		<axis> 
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-0.019</z-m>  
		</center>
	</animation>         

</PropertyList>


