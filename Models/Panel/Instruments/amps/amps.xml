<?xml version="1.0"?>

<PropertyList>


 <name>Ammeter</name>
 <path>amps.ac</path>

    <effect>
        <inherits-from>Aircraft/c170b/Models/Effects/interior/c170b-interior-compartment-1</inherits-from>
        <object-name>face</object-name>
        <object-name>old_needle</object-name>
    </effect>


<animation> 
  <type>rotate</type>
  <object-name>old_needle</object-name>
  <property>systems/c170b/electrical/bus_load</property>
  <interpolation>
		<entry><ind> -30</ind><dep> -32</dep></entry>
		<entry><ind> -15</ind><dep> -16</dep></entry>
		<entry><ind>   0</ind><dep>   0</dep></entry>
		<entry><ind>  15</ind><dep>  16</dep></entry>
		<entry><ind>  30</ind><dep>  32</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>-0.019</z-m>  
  </center>
 </animation>         

</PropertyList>


