<?xml version="1.0"?>

<PropertyList>

    <path>chocks.ac</path>
    <nopreview/>
	
	  <animation>
   <type>select</type>
   <object-name>chockFL</object-name>
   <object-name>chockRL</object-name>
   <object-name>chockFR</object-name>
   <object-name>chockRR</object-name>
   <object-name>cordL</object-name>
   <object-name>cordR</object-name>
   <condition>
     <property>sim/model/c170b/chocks</property>
   </condition>
 </animation>

<animation>
   <type>pick</type>
   <object-name>cordL</object-name>
   <object-name>cordR</object-name>
   <action>
     <button>0</button>
     <repeatable>false</repeatable>
     <binding>
		<command>property-toggle</command>
       <property>sim/model/c170b/chocks</property>
     </binding>
   </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>remove_chocks</tooltip-id>
      <label>Remove chocks</label>
   </binding>
   </hovered>
</animation>

</PropertyList>
